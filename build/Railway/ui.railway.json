{
	"$schema": "https://railway.com/railway.schema.json",
	"build": {
		"builder": "DOCKERFILE",
		"nixpacksPlan": {
			"providers": [
				"..."
			]
		},
		"watchPatterns": [
			"/TeeTimeTally.UI/**",
			"/build/Dockerfiles/UI.Dockerfile",
			"/build/Railway/ui.railway.json"
		],
		"dockerfilePath": "/build/Dockerfiles/UI.Dockerfile"
	},
	"deploy": {
		"runtime": "V2",
		"numReplicas": 1,
		"sleepApplication": false,
		"multiRegionConfig": {
			"us-east4-eqdc4a": {
				"numReplicas": 1
			}
		},
		"restartPolicyType": "ON_FAILURE",
		"restartPolicyMaxRetries": 10
	}
}